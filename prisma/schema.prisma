datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  @@map("users")
  id String @id @default(uuid())
  email String @db.VarChar @unique
  password String @db.VarChar
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
